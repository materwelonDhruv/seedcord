{
  "name": "@seedcord/seedcord",
  "version": "0.1.0",
  "description": "A focused OOP framework for Discord.js bots.",
  "main": "packages/core/core/Seedcord.js",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.12.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "format": "turbo format",
    "format:check": "turbo format:check",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "clean": "turbo clean",
    "rebuild": "turbo clean && turbo build",
    "prebuild": "turbo clean && turbo lint && turbo typecheck",
    "turbo:prune": "turbo prune",
    "turbo:graph": "turbo build --graph",
    "turbo:trace": "turbo build --trace",
    "prepare": "husky",
    "local:core": "cd packages/core && NODE_OPTIONS=--conditions=development tsx watch src/index.ts",
    "local:mock": "cd packages/mock && NODE_OPTIONS=--conditions=development tsx watch src/bot.ts",
    "local:mock:watch": "cd packages/mock && tsx src/start-watch.ts",
    "local:test:core": "cd packages/core && NODE_OPTIONS=--conditions=development vitest",
    "local:test:mock": "cd packages/mock && NODE_OPTIONS=--conditions=development vitest"
  },
  "keywords": [
    "discord",
    "discord.js",
    "bot",
    "framework",
    "oop",
    "typescript"
  ],
  "author": "Dhruv <materwelondhruv@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@seedcord/eslint-config": "workspace:^",
    "@types/chai": "^5.2.2",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/node": "^24.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "chai": "^5.2.1",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "turbo": "^2.5.5",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {},
  "engines": {
    "node": ">=22.12.0"
  }
}
