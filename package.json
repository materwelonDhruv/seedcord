{
  "name": "seedcord",
  "version": "0.1.0",
  "description": "A focused OOP framework for Discord.js bots.",
  "main": "index.js",
  "packageManager": "pnpm@10.12.0",
  "scripts": {
    "start:watch": "tsx start-watch.ts",
    "test": "mocha",
    "lint": "eslint 'src/**/*.{ts,tsx}' --cache",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix --cache",
    "lint:check": "eslint-config-prettier 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,md}' --cache",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,json,md}' --cache",
    "build": "tsc -p .",
    "start:bot": "pnpm run build && node dist/bot.js",
    "dev:bot": "nodemon --config nodemon.json",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean && pnpm run lint && pnpm run typecheck",
    "prepare": "husky"
  },
  "keywords": [
    "discord",
    "discord.js",
    "bot",
    "framework",
    "oop",
    "typescript"
  ],
  "author": "Dhruv <materwelondhruv@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@types/chai": "^5.2.2",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "chai": "^5.2.1",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "mocha": "^11.7.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.0",
    "mongoose": "^8.16.4",
    "reflect-metadata": "^0.2.2",
    "winston": "^3.17.0"
  },
  "engines": {
    "node": ">=22.12.0"
  }
}
