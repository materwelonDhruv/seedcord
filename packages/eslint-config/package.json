{
  "name": "@seedcord/eslint-config",
  "type": "module",
  "version": "1.0.2",
  "description": "ESlint config for Seedcord projects",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/materwelondhruv/seedcord.git",
    "directory": "packages/eslint-config"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./create-config.d.mts",
        "default": "./create-config.mjs"
      },
      "require": {
        "types": "./create-config.d.cts",
        "default": "./create-config.cjs"
      }
    }
  },
  "files": [
    "dist",
    "create-config.mjs",
    "create-config.d.mts",
    "constants.mjs",
    "prettier.mjs",
    "prettier.d.mts",
    "rules/*.mjs"
  ],
  "scripts": {
    "build": "echo 'Already built as .mjs files'",
    "clean": "echo 'No build artifacts to clean'",
    "lint": "eslint '*.{mjs,ts}' --cache",
    "lint:fix": "eslint '*.{mjs,ts}' --fix --cache",
    "fmt": "prettier --write '*.{mjs,ts,json,md}' --cache",
    "fmt:check": "prettier --check '*.{mjs,ts,json,md}' --cache",
    "tc": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest dev",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@types/lodash.merge": "^4.6.9",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "lodash.merge": "^4.6.2",
    "typescript-eslint": "^8.39.1"
  }
}
