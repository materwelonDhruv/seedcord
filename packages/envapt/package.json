{
  "name": "@seedcord/envapt",
  "type": "module",
  "version": "1.0.0",
  "description": "The apt way to handle env",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./envapter": {
      "import": "./dist/Envapter.js",
      "require": "./dist/Envapter.js",
      "types": "./dist/Envapter.d.ts"
    },
    "./envapt": {
      "import": "./dist/Envapt.js",
      "require": "./dist/Envapt.js",
      "types": "./dist/Envapt.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/seedcord/seedcord.git",
    "directory": "packages/envapt"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}' --cache",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix --cache",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,md}' --cache",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,json,md}' --cache",
    "typecheck": "tsc --noEmit",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "env",
    ".env",
    "dotenv",
    "environment",
    "variables",
    "config",
    "configuration",
    "settings"
  ],
  "author": "Dhruv <materwelondhruv@gmail.com>",
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Mason Rogers",
      "email": "viction.dev@gmail.com"
    }
  ],
  "devDependencies": {
    "@seedcord/eslint-config": "workspace:^",
    "@types/chai": "^5.2.2",
    "@types/lodash.invert": "^4.3.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.7",
    "chai": "^5.1.2",
    "eslint": "^9.16.0",
    "mocha": "^11.0.1",
    "prettier": "^3.4.2",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "dependencies": {
    "dotenv": "^17.2.0",
    "lodash.invert": "^4.3.0"
  }
}
