{
    "name": "@seedcord/tsconfig",
    "type": "module",
    "version": "1.1.1",
    "description": "Shared TypeScript configuration for seedcord projects",
    "main": "./base.json",
    "license": "Apache-2.0",
    "keywords": [
        "tsconfig",
        "typescript",
        "seedcord"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/materwelonDhruv/seedcord.git",
        "directory": "packages/tsconfig"
    },
    "exports": {
        ".": {
            "default": "./base.json"
        },
        "./base": {
            "default": "./base.json"
        },
        "./node": {
            "default": "./node.json"
        }
    },
    "files": [
        "base.json",
        "node.json",
        "LICENSE",
        "README.md"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "scripts": {
        "build": "echo 'Are you seriously trying to build .json files?'",
        "clean": "echo 'What build artifacts?'",
        "fmt": "prettier --write '*.{json,md}' --cache",
        "fmt:check": "prettier --check '*.{json,md}' --cache"
    }
}
