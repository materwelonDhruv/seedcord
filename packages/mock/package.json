{
  "name": "@seedcord/mock",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "Mock bot for testing",
  "repository": {
    "type": "git",
    "url": "https://github.com/seedcord/seedcord.git",
    "directory": "packages/mock"
  },
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [],
  "scripts": {
    "dev": "tsx src/bot.ts --conditions=development",
    "dev:bot": "nodemon --config nodemon.json",
    "start:watch": "tsx src/start-watch.ts",
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint 'src/**/*.{ts,tsx}' --cache",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix --cache",
    "fmt": "prettier --write 'src/**/*.{ts,tsx,json,md}' --cache",
    "fmt:check": "prettier --check 'src/**/*.{ts,tsx,json,md}' --cache",
    "tc": "tsc --noEmit",
    "test": "NODE_OPTIONS=--conditions=development vitest run",
    "test:watch": "NODE_OPTIONS=--conditions=development vitest dev",
    "coverage": "NODE_OPTIONS=--conditions=development vitest run --coverage"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@seedcord/types": "workspace:^",
    "discord.js": "^14.21.0",
    "envapt": "^2.2.8",
    "mongoose": "^8.17.1",
    "reflect-metadata": "^0.2.2",
    "seedcord": "workspace:^"
  },
  "devDependencies": {
    "@seedcord/eslint-config": "workspace:^",
    "@seedcord/tsconfig": "workspace:^",
    "@seedcord/tsup-config": "workspace:^"
  }
}
