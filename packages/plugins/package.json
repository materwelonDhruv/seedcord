{
    "name": "@seedcord/plugins",
    "type": "module",
    "version": "0.4.0",
    "description": "Official plugins for Seedcord Discord bot framework",
    "repository": {
        "type": "git",
        "url": "https://github.com/materwelondhruv/seedcord.git",
        "directory": "packages/plugins"
    },
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs",
            "default": "./dist/index.mjs"
        }
    },
    "files": [
        "dist",
        "package.json",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf dist",
        "lint": "eslint 'src/**/*.{ts,tsx}' --cache",
        "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix --cache",
        "fmt": "prettier --write 'src/**/*.{ts,tsx,json,md}' --cache",
        "fmt:check": "prettier --check 'src/**/*.{ts,tsx,json,md}' --cache",
        "tc": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest dev",
        "coverage": "vitest run --coverage"
    },
    "peerDependencies": {
        "typescript": "catalog:peer"
    },
    "dependencies": {
        "@seedcord/types": "workspace:^",
        "@seedcord/utils": "workspace:^",
        "chalk": "catalog:deps",
        "envapt": "catalog:deps",
        "kysely": "^0.28.8",
        "mongoose": "catalog:deps",
        "pg": "^8.16.3",
        "reflect-metadata": "catalog:deps",
        "seedcord": "workspace:^",
        "type-fest": "catalog:deps"
    },
    "devDependencies": {
        "@types/pg": "^8.15.6",
        "@seedcord/eslint-config": "workspace:^",
        "@seedcord/tsconfig": "workspace:^",
        "@seedcord/tsup-config": "workspace:^"
    }
}
